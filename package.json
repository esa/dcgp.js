{
  "name": "dcgp",
  "version": "0.1.0",
  "description": "JavaScript bindings of dcgp compiled to WebAssembly",
  "main": "lib/index.umd.js",
  "module": "lib/dcgp.es.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mikeheddes/dcgp.js.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:cpp": "emcc src/bindings/* -o dcgp.js -I$(pwd)/dcgp/include/ -I$(pwd)/audi/include/ -std=c++11 -g4 --source-map-base http://localhost:8080/ -O3 -s MODULARIZE=1 -s EXPORT_ES6=1 -s EXPORT_NAME=dcgp",
    "build": "rm lib/* && rollup -c"
  },
  "keywords": [
    "dcgp",
    "WebAssembly"
  ],
  "author": "Mike Heddes",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "http-server": "^0.11.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "rollup": "^1.3.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-url": "^2.2.0"
  }
}
